{
    "name": "rapide/laravel-queue-kafka",
    "description": "Kafka driver for Laravel Queue",
    "license": "MIT",
    "type": "library",
    "version": "1.0",
    "authors": [
        {
            "name": "Peter Mein",
            "email": "petermein@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2|>=8.0",
        "illuminate/database": "6.* || 7.* || 8.*",
        "illuminate/support": "6.* || 7.* || 8.*",
        "illuminate/queue": "6.* || 7.* || 8.*",
        "ext-rdkafka": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rapide\\LaravelQueueKafka\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapide\\LaravelQueueKafka\\LaravelQueueKafkaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
